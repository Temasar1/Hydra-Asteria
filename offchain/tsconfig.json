{
  "compilerOptions": {
    "target": "ES2022", // Or "ESNext"
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,             
    "types": [],                    
    "noEmit": true,
  },
  "include": ["**/*.ts", "**/*.json", "**/*.csv"]      
}
